{
  "version": "1.0.1",
  "name": "Pine",
  "manifest_version": 2,
  "description": "Automatically discover spatial data",
  "browser_action": {
    "default_title": "Pine",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [ "webRequest","contextMenus", "tabs", "storage", "*://*/*" ],
  "content_security_policy": ""
}
